<main class="page-content">
  <div class="container">
    <div class="actions-subtitle">
      <h2>Supplier: {{this.model.customId}}</h2>
      <ul class="actions">
        <li>
          <LinkTo @route="tariff-plan.tp-suppliers.tp-supplier.edit">
            <i class="material-icons circle btn btn-primary" data-test-tp-supplier-edit>edit</i>
          </LinkTo>
        </li>
      </ul>
    </div>
    <hr>
    <div class="row">
      <div class="col-12 col-md-3">
        <h5>PK</h5>
        <p>{{this.model.id}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>ID</h5>
        <p>{{this.model.customId}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Tenant</h5>
        <p>{{this.model.tenant}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Filter IDs</h5>
        <p>{{this.model.filterIds}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Sorting</h5>
        <p>{{this.model.sorting}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Sorting parameters</h5>
        <p>{{this.model.sortingParameters}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Supplier ID</h5>
        <p>{{this.model.supplierId}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Supplier Filter IDs</h5>
        <p>{{this.model.supplierFilterIds}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Supplier Account IDs</h5>
        <p>{{this.model.supplierAccountIds}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Supplier Rating plan IDs</h5>
        <p>{{this.model.supplierRatingplanIds}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Supplier Resource IDs</h5>
        <p>{{this.model.supplierResourceIds}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Supplier Stat IDs</h5>
        <p>{{this.model.supplierStatIds}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Supplier Weight</h5>
        <p>{{this.model.supplierWeight}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>ID</h5>
        <p>{{this.model.customId}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Created at</h5>
        <p>{{moment-format model.createdAt}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Blocker</h5>
        <p>
          {{#if model.blocker}}
            <i class="material-icons">check</i>
          {{else}}
            <i class="material-icons">remove</i>
          {{/if}}
        </p>
      </div>
    </div>
  </div>
</main>