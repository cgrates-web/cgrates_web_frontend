<main class="page-content">
  <div class="container">
    <div class="actions-subtitle">
      <h2>TpLcrRule: {{this.model.id}}</h2>
      <ul class="actions">
        <li>
          <LinkTo @route="tariff-plan.tp-lcr-rules.tp-lcr-rule.edit">
            <i class="material-icons circle btn btn-primary" data-test-lcr-rule-edit>edit</i>
          </LinkTo>
        </li>
      </ul>
    </div>
    <hr>
    <div class="row">
      <div class="col-12 col-md-3">
        <h5>Id</h5>
        <p>{{this.model.id}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Direction</h5>
        <p>{{this.model.direction}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Tenant</h5>
        <p>{{this.model.tenant}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Category</h5>
        <p>{{this.model.category}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Account</h5>
        <p>{{this.model.account}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Subject</h5>
        <p>{{this.model.subject}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Destination tag</h5>
        <p>{{this.model.destinationTag}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>RP category</h5>
        <p>{{this.model.rpCategory}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Strategy</h5>
        <p>{{this.model.strategy}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Strategy params</h5>
        <p>{{this.model.strategyParams}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Activation time</h5>
        <p>{{this.model.activationTime}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Weight</h5>
        <p>{{this.model.weight}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Created at</h5>
        <p>{{moment-format model.createdAt}}</p>
      </div>
    </div>
  </div>
</main>