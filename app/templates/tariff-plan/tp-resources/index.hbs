<main class="page-content">
  <div class="container xxl">
    <div class="actions-subtitle">
      <h2>
        TpResources list
      </h2>
      <ul class="actions">
        <TpIndexButtons
          @model={{this.model}}
          @tariffPlanId={{this.tariffPlanId}}
          @permittedFilters={{permittedFilters}}
          @refresh="refresh"
          @controller={{this}}
        />
      </ul>
    </div>
    <TotalRecords @totalRecords={{meta.total_records}} />
    <div class="row">
      <div class="col-md-9">
        <div class="table-responsive">
          <table class="table table-striped">
            <thead>
              <tr>
                <SortHeader
                  @label="ID"
                  @key="id"
                  @sortColumn={{sortColumn}}
                  @sortOrder={{sortOrder}}
                  @sortAction="sortBy"
                />
                <SortHeader
                  @label="Tenant"
                  @key="tenant"
                  @sortColumn={{sortColumn}}
                  @sortOrder={{sortOrder}}
                  @sortAction="sortBy"
                  @data-test-sort-tenant={{true}}
                />
                <SortHeader
                  @label="Custom ID"
                  @key="customId"
                  @sortColumn={{sortColumn}}
                  @sortOrder={{sortOrder}}
                  @sortAction="sortBy"
                />
                <SortHeader
                  @label="Filter IDs"
                  @key="filterIds"
                  @sortColumn={{sortColumn}}
                  @sortOrder={{sortOrder}}
                  @sortAction="sortBy"
                />
                <SortHeader
                  @label="Activation interval"
                  @key="activationInterval"
                  @sortColumn={{sortColumn}}
                  @sortOrder={{sortOrder}}
                  @sortAction="sortBy"
                />
                <SortHeader
                  @label="Usage ttl"
                  @key="usageTtl"
                  @sortColumn={{sortColumn}}
                  @sortOrder={{sortOrder}}
                  @sortAction="sortBy"
                />
                <SortHeader
                  @label="Limit"
                  @key="limit"
                  @sortColumn={{sortColumn}}
                  @sortOrder={{sortOrder}}
                  @sortAction="sortBy"
                />
                <SortHeader
                  @label="Allocation message"
                  @key="allocationMessage"
                  @sortColumn={{sortColumn}}
                  @sortOrder={{sortOrder}}
                  @sortAction="sortBy"
                />
                <SortHeader
                  @label="Blocker"
                  @key="blocker"
                  @sortColumn={{sortColumn}}
                  @sortOrder={{sortOrder}}
                  @sortAction="sortBy"
                />
                <SortHeader
                  @label="Stored"
                  @key="stored"
                  @sortColumn={{sortColumn}}
                  @sortOrder={{sortOrder}}
                  @sortAction="sortBy"
                />
                <SortHeader
                  @label="Weight"
                  @key="weight"
                  @sortColumn={{sortColumn}}
                  @sortOrder={{sortOrder}}
                  @sortAction="sortBy"
                />
                <SortHeader
                  @label="Threshold IDs"
                  @key="thresholdIds"
                  @sortColumn={{sortColumn}}
                  @sortOrder={{sortOrder}}
                  @sortAction="sortBy"
                />
                <SortHeader
                  @label="Created at"
                  @key="createdAt"
                  @sortColumn={{sortColumn}}
                  @sortOrder={{sortOrder}}
                  @sortAction="sortBy"
                />
                <td>
                  Actions
                </td>
              </tr>
            </thead>
            <tbody>
              {{#each model as |tpResource|}}
                <tr>
                  <td>
                    <LinkTo
                      @route="tariff-plan.tp-resources.tp-resource"
                      @model={{tpResource}}
                    >
                      {{tpResource.id}}
                    </LinkTo>
                  </td>
                  <td>
                    <LinkTo
                      @route="tariff-plan.tp-resources.tp-resource"
                      @model={{tpResource}}
                    >
                      {{tpResource.tenant}}
                    </LinkTo>
                  </td>
                  <td>
                    <LinkTo
                      @route="tariff-plan.tp-resources.tp-resource"
                      @model={{tpResource}}
                    >
                      {{tpResource.customId}}
                    </LinkTo>
                  </td>
                  <td>
                    <LinkTo
                      @route="tariff-plan.tp-resources.tp-resource"
                      @model={{tpResource}}
                    >
                      {{tpResource.filterIds}}
                    </LinkTo>
                  </td>
                  <td>
                    <LinkTo
                      @route="tariff-plan.tp-resources.tp-resource"
                      @model={{tpResource}}
                    >
                      {{tpResource.activationInterval}}
                    </LinkTo>
                  </td>
                  <td>
                    <LinkTo
                      @route="tariff-plan.tp-resources.tp-resource"
                      @model={{tpResource}}
                    >
                      {{tpResource.usageTtl}}
                    </LinkTo>
                  </td>
                  <td>
                    <LinkTo
                      @route="tariff-plan.tp-resources.tp-resource"
                      @model={{tpResource}}
                    >
                      {{tpResource.limit}}
                    </LinkTo>
                  </td>
                  <td>
                    <LinkTo
                      @route="tariff-plan.tp-resources.tp-resource"
                      @model={{tpResource}}
                    >
                      {{tpResource.allocationMessage}}
                    </LinkTo>
                  </td>
                  <td>
                    <LinkTo
                      @route="tariff-plan.tp-resources.tp-resource"
                      @model={{tpResource}}
                    >
                      {{tpResource.blocker}}
                    </LinkTo>
                  </td>
                  <td>
                    <LinkTo
                      @route="tariff-plan.tp-resources.tp-resource"
                      @model={{tpResource}}
                    >
                      {{tpResource.stored}}
                    </LinkTo>
                  </td>
                  <td>
                    <LinkTo
                      @route="tariff-plan.tp-resources.tp-resource"
                      @model={{tpResource}}
                    >
                      {{tpResource.weight}}
                    </LinkTo>
                  </td>
                  <td>
                    <LinkTo
                      @route="tariff-plan.tp-resources.tp-resource"
                      @model={{tpResource}}
                    >
                      {{tpResource.thresholdIds}}
                    </LinkTo>
                  </td>
                  <td>
                    <LinkTo
                      @route="tariff-plan.tp-resources.tp-resource"
                      @model={{tpResource}}
                    >
                      {{moment-format tpResource.createdAt}}
                    </LinkTo>
                  </td>
                  <td class="actions">
                    <LinkTo
                      @route="tariff-plan.tp-resources.tp-resource.edit"
                      @model={{tpResource}}
                    >
                      <i
                        class="material-icons circle btn btn-primary"
                        data-test-resource-edit
                      >
                        edit
                      </i>
                    </LinkTo>
                    <a
                      class="ml-1"
                      role="button"
                      {{action "remove" tpResource}}
                    >
                      <i
                        class="material-icons circle btn btn-danger"
                        data-test-resource-remove
                      >
                        remove
                      </i>
                    </a>
                  </td>
                </tr>
              {{/each}}
            </tbody>
          </table>
        </div>
        <PageNumbers
          @content={{pagination}}
          @action="toPage"
          @showFL={{true}}
          @numPagesToShow={{4}}
        />
      </div>
      <div class="col-md-3">
        <FiltersBlock @search={{action "search"}} as |filtersBlock|>
          <FilterText
            @label="Tenant"
            @key="tenant"
            @value={{tenant}}
            @onValueChange={{action "pushValue" target=filtersBlock}}
            @data-test-filter-tenant={{true}}
          />
          <FilterText
            @label="Custom ID"
            @key="customId"
            @value={{customId}}
            @onValueChange={{action "pushValue" target=filtersBlock}}
            @data-test-filter-customid={{true}}
          />
          <FilterText
            @label="Filter IDs"
            @key="filterIds"
            @value={{filterIds}}
            @onValueChange={{action "pushValue" target=filtersBlock}}
            @data-test-filter-filter-ids={{true}}
          />
          <FilterText
            @label="Threshold IDs"
            @key="thresholdIds"
            @value={{thresholdIds}}
            @onValueChange={{action "pushValue" target=filtersBlock}}
            @data-test-filter-threshold-ids={{true}}
          />
          <FilterText
            @label="Activation interval"
            @key="activationInterval"
            @value={{activationInterval}}
            @onValueChange={{action "pushValue" target=filtersBlock}}
            @data-test-filter-activation-interval={{true}}
          />
          <FilterText
            @label="Usage ttl"
            @key="usageTtl"
            @value={{usageTtl}}
            @onValueChange={{action "pushValue" target=filtersBlock}}
            @data-test-filter-usage-ttl={{true}}
          />
          <FilterText
            @label="Limit"
            @key="limit"
            @value={{limit}}
            @onValueChange={{action "pushValue" target=filtersBlock}}
            @data-test-filter-limit={{true}}
          />
          <FilterText
            @label="Allocation message"
            @key="allocationMessage"
            @value={{allocationMessage}}
            @onValueChange={{action "pushValue" target=filtersBlock}}
            @data-test-filter-allocation-message={{true}}
          />
          <FilterSelect
            @label="Blocker"
            @key="blocker"
            @value={{blocker}}
            @onValueChange={{action "pushValue" target=filtersBlock}}
            @content={{selectValues.boolean}}
            @data-test-filter-blocker={{true}}
          />
          <FilterSelect
            @label="Stored"
            @key="stored"
            @value={{stored}}
            @onValueChange={{action "pushValue" target=filtersBlock}}
            @content={{selectValues.boolean}}
            @data-test-filter-stored={{true}}
          />
          <FilterNumber
            @label="Weight"
            @key="weight"
            @value={{weight}}
            @onValueChange={{action "pushValue" target=filtersBlock}}
            @step="0.1"
            @min="0"
            @data-test-filter-weight={{true}}
          />
        </FiltersBlock>
      </div>
    </div>
  </div>
</main>