<main class="page-content">
  <div class="container xxl">
    <div class="actions-subtitle">
      <h2>
        TpCdrStats list
      </h2>
      <ul class="actions">
        <TpIndexButtons
          @model={{this.model}}
          @tariffPlanId={{this.tariffPlanId}}
          @permittedFilters={{permittedFilters}}
          @refresh="refresh"
          @controller={{this}}
        />
      </ul>
    </div>
    <TotalRecords @totalRecords={{meta.total_records}} />
    <div class="row">
      <div class="col-md-9">
        <div class="table-responsive">
          <table class="table table-striped table-sm">
            <thead>
              <tr>
                <SortHeader
                  @label="ID"
                  @key="id"
                  @sortColumn={{sortColumn}}
                  @sortOrder={{sortOrder}}
                  @sortAction="sortBy"
                />
                <SortHeader
                  @label="Tag"
                  @key="tag"
                  @sortColumn={{sortColumn}}
                  @sortOrder={{sortOrder}}
                  @sortAction="sortBy"
                  @data-test-sort-tag={{true}}
                />
                <SortHeader
                  @label="Metrics"
                  @key="metrics"
                  @sortColumn={{sortColumn}}
                  @sortOrder={{sortOrder}}
                  @sortAction="sortBy"
                />
                <SortHeader
                  @label="Tors"
                  @key="tors"
                  @sortColumn={{sortColumn}}
                  @sortOrder={{sortOrder}}
                  @sortAction="sortBy"
                />
                <SortHeader
                  @label="Cdr hosts"
                  @key="cdrHosts"
                  @sortColumn={{sortColumn}}
                  @sortOrder={{sortOrder}}
                  @sortAction="sortBy"
                />
                <SortHeader
                  @label="Cdr sources"
                  @key="cdrSources"
                  @sortColumn={{sortColumn}}
                  @sortOrder={{sortOrder}}
                  @sortAction="sortBy"
                />
                <SortHeader
                  @label="Req types"
                  @key="reqTypes"
                  @sortColumn={{sortColumn}}
                  @sortOrder={{sortOrder}}
                  @sortAction="sortBy"
                />
                <SortHeader
                  @label="Directions"
                  @key="directions"
                  @sortColumn={{sortColumn}}
                  @sortOrder={{sortOrder}}
                  @sortAction="sortBy"
                />
                <SortHeader
                  @label="Tenants"
                  @key="tenants"
                  @sortColumn={{sortColumn}}
                  @sortOrder={{sortOrder}}
                  @sortAction="sortBy"
                />
                <SortHeader
                  @label="Categories"
                  @key="categories"
                  @sortColumn={{sortColumn}}
                  @sortOrder={{sortOrder}}
                  @sortAction="sortBy"
                />
                <SortHeader
                  @label="Accounts"
                  @key="accounts"
                  @sortColumn={{sortColumn}}
                  @sortOrder={{sortOrder}}
                  @sortAction="sortBy"
                />
                <SortHeader
                  @label="Subjects"
                  @key="subjects"
                  @sortColumn={{sortColumn}}
                  @sortOrder={{sortOrder}}
                  @sortAction="sortBy"
                />
                <SortHeader
                  @label="Destination IDs"
                  @key="destinationIds"
                  @sortColumn={{sortColumn}}
                  @sortOrder={{sortOrder}}
                  @sortAction="sortBy"
                />
                <SortHeader
                  @label="Suppliers"
                  @key="suppliers"
                  @sortColumn={{sortColumn}}
                  @sortOrder={{sortOrder}}
                  @sortAction="sortBy"
                />
                <SortHeader
                  @label="Action triggers"
                  @key="actionTriggers"
                  @sortColumn={{sortColumn}}
                  @sortOrder={{sortOrder}}
                  @sortAction="sortBy"
                />
                <SortHeader
                  @label="Created at"
                  @key="createdAt"
                  @sortColumn={{sortColumn}}
                  @sortOrder={{sortOrder}}
                  @sortAction="sortBy"
                />
                <td>
                  Actions
                </td>
              </tr>
            </thead>
            <tbody>
              {{#each model as |tpCdrStat|}}
                <tr>
                  <td>
                    <LinkTo
                      @route="tariff-plan.tp-cdr-stats.tp-cdr-stat"
                      @model={{tpCdrStat}}
                    >
                      {{tpCdrStat.id}}
                    </LinkTo>
                  </td>
                  <td>
                    <LinkTo
                      @route="tariff-plan.tp-cdr-stats.tp-cdr-stat"
                      @model={{tpCdrStat}}
                    >
                      {{tpCdrStat.tag}}
                    </LinkTo>
                  </td>
                  <td>
                    <LinkTo
                      @route="tariff-plan.tp-cdr-stats.tp-cdr-stat"
                      @model={{tpCdrStat}}
                    >
                      {{tpCdrStat.metrics}}
                    </LinkTo>
                  </td>
                  <td>
                    <LinkTo
                      @route="tariff-plan.tp-cdr-stats.tp-cdr-stat"
                      @model={{tpCdrStat}}
                    >
                      {{tpCdrStat.tors}}
                    </LinkTo>
                  </td>
                  <td>
                    <LinkTo
                      @route="tariff-plan.tp-cdr-stats.tp-cdr-stat"
                      @model={{tpCdrStat}}
                    >
                      {{tpCdrStat.cdrHosts}}
                    </LinkTo>
                  </td>
                  <td>
                    <LinkTo
                      @route="tariff-plan.tp-cdr-stats.tp-cdr-stat"
                      @model={{tpCdrStat}}
                    >
                      {{tpCdrStat.cdrSources}}
                    </LinkTo>
                  </td>
                  <td>
                    <LinkTo
                      @route="tariff-plan.tp-cdr-stats.tp-cdr-stat"
                      @model={{tpCdrStat}}
                    >
                      {{tpCdrStat.reqTypes}}
                    </LinkTo>
                  </td>
                  <td>
                    <LinkTo
                      @route="tariff-plan.tp-cdr-stats.tp-cdr-stat"
                      @model={{tpCdrStat}}
                    >
                      {{tpCdrStat.directions}}
                    </LinkTo>
                  </td>
                  <td>
                    <LinkTo
                      @route="tariff-plan.tp-cdr-stats.tp-cdr-stat"
                      @model={{tpCdrStat}}
                    >
                      {{tpCdrStat.tenants}}
                    </LinkTo>
                  </td>
                  <td>
                    <LinkTo
                      @route="tariff-plan.tp-cdr-stats.tp-cdr-stat"
                      @model={{tpCdrStat}}
                    >
                      {{tpCdrStat.categories}}
                    </LinkTo>
                  </td>
                  <td>
                    <LinkTo
                      @route="tariff-plan.tp-cdr-stats.tp-cdr-stat"
                      @model={{tpCdrStat}}
                    >
                      {{tpCdrStat.accounts}}
                    </LinkTo>
                  </td>
                  <td>
                    <LinkTo
                      @route="tariff-plan.tp-cdr-stats.tp-cdr-stat"
                      @model={{tpCdrStat}}
                    >
                      {{tpCdrStat.subjects}}
                    </LinkTo>
                  </td>
                  <td>
                    <LinkTo
                      @route="tariff-plan.tp-cdr-stats.tp-cdr-stat"
                      @model={{tpCdrStat}}
                    >
                      {{tpCdrStat.destinationIds}}
                    </LinkTo>
                  </td>
                  <td>
                    <LinkTo
                      @route="tariff-plan.tp-cdr-stats.tp-cdr-stat"
                      @model={{tpCdrStat}}
                    >
                      {{tpCdrStat.suppliers}}
                    </LinkTo>
                  </td>
                  <td>
                    <LinkTo
                      @route="tariff-plan.tp-cdr-stats.tp-cdr-stat"
                      @model={{tpCdrStat}}
                    >
                      {{tpCdrStat.actionTriggers}}
                    </LinkTo>
                  </td>
                  <td>
                    <LinkTo
                      @route="tariff-plan.tp-cdr-stats.tp-cdr-stat"
                      @model={{tpCdrStat}}
                    >
                      {{moment-format tpCdrStat.createdAt}}
                    </LinkTo>
                  </td>
                  <td class="actions">
                    <LinkTo
                      @route="tariff-plan.tp-cdr-stats.tp-cdr-stat.edit"
                      @model={{tpCdrStat}}
                    >
                      <i
                        class="material-icons circle btn btn-primary"
                        data-test-tp-cdr-stat-edit
                      >
                        edit
                      </i>
                    </LinkTo>
                    <a class="ml-1" role="button" {{action "remove" tpCdrStat}}>
                      <i
                        class="material-icons circle btn btn-danger"
                        data-test-tp-cdr-stat-remove
                      >
                        remove
                      </i>
                    </a>
                  </td>
                </tr>
              {{/each}}
            </tbody>
          </table>
        </div>
        <PageNumbers
          @content={{this.pagination}}
          @action={{action "toPage"}}
          @numPagesToShow={{4}}
        />
      </div>
      <div class="col-md-3">
        <FiltersBlock
          @search={{action "search"}}
          @activeFilters={{this.filters}} as |filtersBlock|
        >
          <FilterText
            @label="Tag"
            @value={{filtersBlock.activeFilters.tag}}
            data-test-filter-tag
          />
          <FilterSelect
            @label="Metrics"
            @value={{filtersBlock.activeFilters.metrics}}
            @content={{selectValues.metrics}}
            @data-test-filter-metrics={{true}}
          />
          <FilterText
            @label="Tors"
            @value={{filtersBlock.activeFilters.tors}}
            data-test-filter-tors
          />
          <FilterText
            @label="Cdr hosts"
            @value={{filtersBlock.activeFilters.cdrHosts}}
            data-test-filter-cdr-hosts
          />
          <FilterText
            @label="Cdr sources"
            @value={{filtersBlock.activeFilters.cdrSources}}
            data-test-filter-cdr-sources
          />
          <FilterText
            @label="Req types"
            @value={{filtersBlock.activeFilters.reqTypes}}
            data-test-filter-req-types
          />
          <FilterText
            @label="Directions"
            @value={{filtersBlock.activeFilters.directions}}
            data-test-filter-directions
          />
          <FilterText
            @label="Tenants"
            @value={{filtersBlock.activeFilters.tenants}}
            data-test-filter-tenants
          />
          <FilterText
            @label="Categories"
            @value={{filtersBlock.activeFilters.categories}}
            data-test-filter-categories
          />
          <FilterText
            @label="Accounts"
            @value={{filtersBlock.activeFilters.accounts}}
            data-test-filter-accounts
          />
          <FilterText
            @label="Subjects"
            @value={{filtersBlock.activeFilters.subjects}}
            data-test-filter-subjects
          />
          <FilterText
            @label="Suppliers"
            @value={{filtersBlock.activeFilters.suppliers}}
            data-test-filter-suppliers
          />
          <FilterSelectSearchToString
            @label="Destination IDs"
            @value={{filtersBlock.activeFilters.destinationIds}}
            @searchModel="tp-destination"
            @searchField="tag"
            @tpid={{this.tariffPlanId}}
            @multiple={{true}}
            @data-test-filter-destination-ids={{true}}
          />
          <FilterSelectSearchToString
            @label="Action triggers"
            @value={{filtersBlock.activeFilters.actionTriggers}}
            @searchModel="tp-action-trigger"
            @searchField="tag"
            @tpid={{this.tariffPlanId}}
            @multiple={{true}}
            @data-test-filter-action-triggers={{true}}
          />
          <FilterCollapseBtn
            @label="Other filters"
            @collapsed={{otherFiltersCollapsed}}
          />
          <BsCollapse @collapsed={{otherFiltersCollapsed}}>
            <FilterNumber
              @label="Queue length"
              @value={{filtersBlock.activeFilters.queueLength}}
              @onValueChange={{filtersBlock.pushValue}}
              @min="0"
              data-test-filter-queue-length
            />
            <FilterNumber
              @label="Time window"
              @value={{filtersBlock.activeFilters.timeWindow}}
              @min="0"
              data-test-filter-time-window
            />
            <FilterNumber
              @label="Save interval"
              @value={{filtersBlock.activeFilters.saveInterval}}
              @min="0"
              data-test-filter-save-interval
            />
            <FilterText
              @label="Setup interval"
              @value={{filtersBlock.activeFilters.setupInterval}}
              data-test-filter-setup-interval
            />
            <FilterText
              @label="Pdd interval"
              @value={{filtersBlock.activeFilters.pddInterval}}
              data-test-filter-pdd-interval
            />
            <FilterText
              @label="Usage interval"
              @value={{filtersBlock.activeFilters.usageInterval}}
              data-test-filter-usage-interval
            />
            <FilterText
              @label="Disconnect causes"
              @value={{filtersBlock.activeFilters.disconnectCauses}}
              data-test-filter-disconnect-causes
            />
            <FilterText
              @label="Mediation run IDs"
              @value={{filtersBlock.activeFilters.mediationRunids}}
              data-test-filter-mediation-runids
            />
            <FilterText
              @label="Rated accounts"
              @value={{filtersBlock.activeFilters.ratedAccounts}}
              data-test-filter-rated-accounts
            />
            <FilterText
              @label="Rated subjects"
              @value={{filtersBlock.activeFilters.ratedSubjects}}
              data-test-filter-rated-subjects
            />
            <FilterText
              @label="Cost interval"
              @value={{filtersBlock.activeFilters.costInterval}}
              data-test-filter-cost-interval
            />
          </BsCollapse>
        </FiltersBlock>
      </div>
    </div>
  </div>
</main>