<main class="page-content">
  <div class="container">
    <div class="actions-subtitle">
      <h2>Timing: {{this.model.tag}}</h2>
      <ul class="actions">
        <li>
          <LinkTo @route="tariff-plan.tp-timings.tp-timing.edit" @model={{this.model}}>
            <i class="material-icons circle btn btn-primary" data-test-tp-timing>edit</i>
          </LinkTo>
        </li>
      </ul>
    </div>
    <hr>
    <div class="row">
      <div class="col-12 col-md-3">
        <h5>Id</h5>
        <p>{{this.model.id}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Tag</h5>
        <p>{{this.model.tag}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Years</h5>
        <p>{{this.model.years}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Months</h5>
        <p>{{this.model.months}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Month Days</h5>
        <p>{{this.model.monthDays}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Week Days</h5>
        <p>{{this.model.weekDays}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Time</h5>
        <p>{{this.model.time}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Created at</h5>
        <p>{{moment-format model.createdAt}}</p>
      </div>
    </div>
  </div>
</main>