<main class="page-content">
  <div class="container">
    <div class="actions-subtitle">
      <h2>TpAction: {{this.model.tag}}</h2>
      <ul class="actions">
        <li>
          <LinkTo @route="tariff-plan.tp-actions.tp-action.edit" @model={{this.model}}>
            <i class="material-icons circle btn btn-primary" data-test-edit>edit</i>
          </LinkTo>
        </li>
      </ul>
    </div>
    <hr>
    <div class="row">
      <div class="col-12 col-md-3">
        <h5>Id</h5>
        <p>{{this.model.id}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Tag</h5>
        <p>{{this.model.tag}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Action</h5>
        <p>{{this.model.action}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Balance tag</h5>
        <p>{{this.model.balanceTag}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Balance type</h5>
        <p>{{this.model.balanceType}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Units</h5>
        <p>{{this.model.units}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Expiry time</h5>
        <p>{{this.model.expiryTime}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Timing tags</h5>
        <p>{{this.model.timingTags}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Destination tags</h5>
        <p>{{this.model.destinationTags}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Rating subject</h5>
        <p>{{this.model.ratingSubject}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Categories</h5>
        <p>{{this.model.categories}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Shared groups</h5>
        <p>{{this.model.sharedGroups}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Balance weight</h5>
        <p>{{this.model.balanceWeight}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Balance blocker</h5>
        <p>{{this.model.balanceBlocker}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Balance disabled</h5>
        <p>{{this.model.balanceDisabled}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Extra parameters</h5>
        <p>{{this.model.extraParameters}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Filter</h5>
        <p>{{this.model.filter}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Weight</h5>
        <p>{{this.model.weight}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Created at</h5>
        <p>{{moment-format model.createdAt}}</p>
      </div>
    </div>
  </div>
</main>