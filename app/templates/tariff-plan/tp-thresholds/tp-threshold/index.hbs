<main class="page-content">
  <div class="container">
    <div class="actions-subtitle">
      <h2>TpThreshold: {{model.customId}}</h2>
      <ul class="actions">
        <li>
          {{#link-to "tariff-plan.tp-thresholds.tp-threshold.edit"}}
            <i class="material-icons circle btn btn-primary" data-test-edit>edit</i>
          {{/link-to}}
        </li>
      </ul>
    </div>
    <hr>
    <div class="row">
      <div class="col-12 col-md-3">
        <h5>ID</h5>
        <p>{{model.id}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Tenant</h5>
        <p>{{model.tenant}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Custom ID</h5>
        <p>{{model.customId}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Filter IDs</h5>
        <p>{{model.filterIds}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Action IDs</h5>
        <p>{{model.actionIds}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Activation interval</h5>
        <p>{{model.activationInterval}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Max hits</h5>
        <p>{{model.maxHits}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Min hits</h5>
        <p>{{model.minHits}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Min sleep</h5>
        <p>{{model.minSleep}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Blocker</h5>
        <p>{{model.blocker}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Async</h5>
        <p>{{model.async}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Weight</h5>
        <p>{{model.weight}}</p>
      </div>
      <div class="col-12 col-md-3">
        <h5>Created at</h5>
        <p>{{moment-format model.createdAt}}</p>
      </div>
    </div>
  </div>
</main>